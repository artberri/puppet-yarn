{
  "name": "artberri-yarn",
  "version": "1.0.0",
  "author": "artberri",
  "summary": "Puppet Module to install Yarn Package Manager",
  "description": "A puppet module to install Yarn a dependency manager for npm and bower.",
  "license": "Apache-2.0",
  "source": "https://github.com/artberri/puppet-yarn",
  "project_page": "https://github.com/artberri/puppet-yarn",
  "issues_url": "https://github.com/artberri/puppet-yarn/issues",
  "tags": ["yarn", "nodejs", "node.js", "node", "npm", "bower"],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "ArchLinux"
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "10"
      ]
    },
    {
      "operatingsystem": "SLES",
      "operatingsystemrelease": [
        "11",
        "12",
        "13"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=3.2.0 <5.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 1.8.0 <3.0.0"
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": ">=2.7.20 <5.0.0"
    }
  ]
}

